      <eStructuralFeatures xsi:type="ecore:EReference" name="taster" upperBound="-1"
          eType="#//bsw/Taster" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MotorHandler" eSuperTypes="#//BaseClass">
      <eStructuralFeatures xsi:type="ecore:EReference" name="motor" upperBound="-1"
          eType="#//bsw/Motor" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Motor" eSuperTypes="#//bsw/ActuatorSensor"/>
    <eClassifiers xsi:type="ecore:EClass" name="UltraschallHandler" eSuperTypes="#//BaseClass">
      <eStructuralFeatures xsi:type="ecore:EReference" name="ultraschall" upperBound="-1"
